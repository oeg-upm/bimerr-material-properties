    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#description -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#description">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Description of the entity.</rdfs:comment>
        <rdfs:label xml:lang="en">description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#identifierInDatabase -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#identifierInDatabase">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A relationship to indicate the identifier used in the source database</rdfs:comment>
        <rdfs:label xml:lang="en">identifier in database</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#ifcIdentifier -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#ifcIdentifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Ifc identifier of the element in an IFC file.</rdfs:comment>
        <rdfs:label xml:lang="en">ifc identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#name -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#name">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Name of the entity.</rdfs:comment>
        <rdfs:label xml:lang="en">name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#position -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#position">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Position of the layer.</rdfs:comment>
        <rdfs:label xml:lang="en">position</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#thickness -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#thickness">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">Thickness of the layer.</rdfs:comment>
        <rdfs:label xml:lang="en">thickness</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#type -->


    <owl:DatatypeProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#type">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Type of material layer.</rdfs:comment>
        <rdfs:label xml:lang="en">type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/core/hasValue -->


    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core/hasValue">
        <rdfs:comment xml:lang="en">A relationship defining the value of a certain property, e.g., energy or power. Note that, even if numeric values are expected to enable reasoning, measurement values could use other datatypes.</rdfs:comment>
        <rdfs:label xml:lang="en">has value</rdfs:label>
    </owl:DatatypeProperty>
    


