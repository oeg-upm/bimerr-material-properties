        <dc:title xml:lang="en">Material properties ontology</dc:title>
        <vann:preferredNamespacePrefix>props</vann:preferredNamespacePrefix>
        <owl:versionInfo>0.0.1</owl:versionInfo>
    </owl:Ontology>
    


