    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterial -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterial">
        <rdfs:comment xml:lang="en">Relation between a construction component (Layer, profile, constituent) and the materials that define them.</rdfs:comment>
        <rdfs:label xml:lang="en">has material</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialConstituent -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialConstituent">
        <rdfs:comment xml:lang="en">A relation between a constituent set and the specific material constituents.</rdfs:comment>
        <rdfs:label xml:lang="en">has material constituent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialConstituentSet -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialConstituentSet">
        <rdfs:comment xml:lang="en">Relation between an element and its construction based in material constituents.</rdfs:comment>
        <rdfs:label xml:lang="en">has material constituent set</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayer -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayer">
        <rdfs:comment xml:lang="en">A relation between a layer set and the specific material layers.</rdfs:comment>
        <rdfs:label xml:lang="en">has material layer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayerSet -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayerSet">
        <rdfs:comment xml:lang="en">Relation between an element and its construction based in material layers.</rdfs:comment>
        <rdfs:label xml:lang="en">has material layer set</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialProfile -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialProfile">
        <rdfs:comment xml:lang="en">A relation between a profile set and the specific material profiles.</rdfs:comment>
        <rdfs:label xml:lang="en">has material profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialProfileSet -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialProfileSet">
        <rdfs:comment xml:lang="en">Relation between an element and its construction based in material profiles.</rdfs:comment>
        <rdfs:label xml:lang="en">has material profile set</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasProperty -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasProperty">
        <rdfs:comment xml:lang="en">A relationship between a feature of interest and a property of that feature.</rdfs:comment>
        <rdfs:label xml:lang="en">has property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasPropertySet -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasPropertySet">
        <rdfs:comment xml:lang="en">Relationship to connect a material or building element to an specific set of properties.</rdfs:comment>
        <rdfs:label xml:lang="en">has property set</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#isMeasurementOfMaterial -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#isMeasurementOfMaterial">
        <rdfs:comment xml:lang="en">Relation between a measurement with a specific material.</rdfs:comment>
        <rdfs:label xml:lang="en">is measurement of material</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/isMeasuredIn -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredIn">
        <rdfs:comment xml:lang="en">A relationship identifying the unit of measure used for a certain entity.</rdfs:comment>
        <rdfs:label xml:lang="en">is measured in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/relatesToMeasurement -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToMeasurement">
        <rdfs:comment xml:lang="en">A relationship between a property and the measurements it relates to.</rdfs:comment>
        <rdfs:label xml:lang="en">relates to measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/relatesToProperty -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToProperty">
        <rdfs:comment xml:lang="en">A relationship between a measurement and the property it relates to.</rdfs:comment>
        <rdfs:label xml:lang="en">relates to property</rdfs:label>
    </owl:ObjectProperty>
    


